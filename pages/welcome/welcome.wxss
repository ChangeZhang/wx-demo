page {
    height: 100%;
    background: #b3d4db;
}
.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 100%;
    justify-content: space-around;
}
.user-avatar {
    width: 200rpx;
    height: 200rpx;
    border-radius: 50%;
}
.wel-text {
    text-align: center;
    font-size: 32rpx;
}
.wel-btn {}
.wel-btn-text {
    display: block;
    border-radius: 5rpx;
    font-size: 24rpx;
    line-height: 60rpx;
    border:1px solid #405f80;
    padding:0 15rpx;
    color: #405f80;
}